<?xml version="1.0"?>
<schema version="0.3">
    <table name="btManualNav">
        <field name="bID" type="I">
            <KEY/>
            <UNSIGNED/>
        </field>
        <field name="navigationType" type="I">
            <UNSIGNED />
            <default value="0" />
        </field>
        <field name="displayImage" type="I">
            <UNSIGNED />
            <default value="0" />
        </field>
    </table>
    <table name="btManualNavEntries">
        <field name="id" type="I">
            <UNSIGNED/>
            <key/>
            <AUTOINCREMENT/>
        </field>
        <field name="bID" type="I">
            <UNSIGNED />
        </field>
        <field name="fID" type="I">
            <unsigned/>
            <default value="0"/>
        </field>
        <field name="cID" type="I">
            <UNSIGNED />
            <default value="0" />
        </field>
        <field name="linkURL"  type="C" size="255"/>
        <field name="internalLinkCID"  type="I">
            <unsigned />
            <default value="0" />
        </field>
        <field name="title" type="X2"></field>
        <field name="sortOrder" type="I"></field>
        <field name="openInNewWindow" type="L">
            <unsigned/>
            <default value="0"/>
            <notnull/>
        </field>
    </table>
</schema>